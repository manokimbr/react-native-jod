{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["ES2021", "DOM"],
    "strict": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "types": [
      "react",
      "react-native",
      "node"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src"
  ],
  "exclude": [
    "node_modules",
    "memory"
  ]
}
